var a = prompt("Podaj liczbe:")

var b = function gx() {
  return this;
}

var c = function fx() {
  return this.a;
}

function silnia(n) {
   if ((n == 0) || (n == 1))
      return 1
   else {
      var result = (n * silnia(n-1) );
      return result
   }
}

console.log(b().a);
console.log(c());
console.log ('Silnia z '+a+' = '+silnia(a));
